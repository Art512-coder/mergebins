{
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "css.customData": [".vscode/css_custom_data.json"],
  "tailwindCSS.experimental.classRegex": [
    "class\\s*:\\s*[\"']([^\"']*)[\"']",
    "className\\s*:\\s*[\"']([^\"']*)[\"']",
    "class=[\"']([^\"']*)[\"']",
    "className=[\"']([^\"']*)[\"']"
  ],
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "emmet.includeLanguages": {
    "vue-html": "html",
    "vue": "html"
  },
  "tailwindCSS.includeLanguages": {
    "vue": "html",
    "vue-html": "html"
  }
}
